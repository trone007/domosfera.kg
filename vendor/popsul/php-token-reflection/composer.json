{
    "name": "popsul/php-token-reflection",
    "type": "library",
    "description": "Library emulating the PHP internal reflection using just the tokenized source code.",
    "keywords": ["library", "tokenizer", "reflection"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ondřej Nešpor",
            "homepage": "https://github.com/Andrewsville"
        },
        {
            "name": "Jaroslav Hanslík",
            "homepage": "https://github.com/kukulich"
        },
        {
            "name": "Boris Momčilović",
            "homepage": "https://github.com/kornrunner"
        },
        {
            "name": "Viktor Suprun",
            "homepage": "https://github.com/POPSuL"
        }
    ],
    "replace": {
        "andrewsville/php-token-reflection": "*"
    },
    "require": {
        "php": ">=5.3.0",
        "ext-tokenizer": "*"
    },
    "autoload": {
        "psr-0": {
            "TokenReflection": "./"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    }
}
