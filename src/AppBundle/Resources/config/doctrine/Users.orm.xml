<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Users" table="users">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="users_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="username" type="text" column="username" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="text" column="password" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="text" column="name" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="surname" type="text" column="surname" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="roles" type="text" column="roles" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
