<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Companion" table="companion">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="companion_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="complect" type="text" column="complect" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="vendorCode" type="text" column="vendor_code" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="text" column="name" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
