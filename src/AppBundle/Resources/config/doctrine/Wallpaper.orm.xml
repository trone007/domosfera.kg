<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Wallpaper" table="wallpaper">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="wallpaper_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="shop" type="text" column="shop" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="organization" type="text" column="organization" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="vendorCode" type="text" column="vendor_code" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="image" type="text" column="image" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="seamlessStructure" type="boolean" column="seamless_structure" nullable="true"/>
    <field name="priceOld" type="decimal" column="price_old" precision="10" scale="0" nullable="true"/>
    <field name="price" type="decimal" column="price" precision="10" scale="0" nullable="true"/>
    <field name="currency" type="text" column="currency" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="unit" type="text" column="unit" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mainNomenclature" type="text" column="main_nomenclature" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomenclature" type="text" column="nomenclature" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="text" column="country" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="catalog" type="text" column="catalog" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="notebook" type="text" column="notebook" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="glitter" type="boolean" column="glitter" nullable="true"/>
    <field name="color1" type="text" column="color_1" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="color2" type="text" column="color_2" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="text" column="type" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="basis" type="text" column="basis" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="picture" type="text" column="picture" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="texture" type="text" column="texture" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="speed" type="decimal" column="speed" precision="10" scale="0" nullable="true"/>
    <field name="uuid" type="text" column="uuid" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="manufacturer" type="text" column="manufacturer" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="successfull" type="decimal" column="successfull" precision="10" scale="5" nullable="true"/>
    <field name="points" type="decimal" column="points" precision="10" scale="5" nullable="true"/>
    <field name="dateTime" type="datetime" column="date_time" nullable="true"/>
    <field name="size" type="decimal" column="size" precision="10" scale="2" nullable="true"/>
    <field name="marketPlan" type="decimal" column="market_plan" precision="10" scale="5" nullable="true"/>
    <field name="style" type="text" column="style" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
